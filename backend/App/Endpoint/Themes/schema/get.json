{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "number"
        },
        "user_id": {
            "type": "number"
        },
        "is_favorite": {
            "type": "boolean"
        },
        "tags": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "number"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        },
        "name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "alternative_names": {
            "type": "array",
            "items": {
                "type": "string",
                "minLength": 1,
                "maxLength": 255
            }
        },
        "reference": {
            "type": "object",
            "properties": {
                "url": {
                    "type": "string"
                }
            },
            "required": [
                "url"
            ]
        },
        "created_at": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": [
        "tags",
        "name",
        "reference",
        "is_favorite"
    ],
    "type": "object"
}