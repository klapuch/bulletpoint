{
  "name": "klapuch/bulletpoint",
  "description": "REST API for bulletpoint",
  "homepage": "https://github.com/klapuch/bulletpoint",
  "type": "project",
  "license": "proprietary",
  "authors": [
    {
      "name": "Dominik Klapuch",
      "email": "klapuchdominik@gmail.com",
      "homepage": "https://www.github.com/klapuch",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-apcu": "*",
    "ext-xsl": "*",
    "ext-igbinary": "*",
    "ext-curl": "*",
    "klapuch/application": "^0.0.4",
    "klapuch/configuration": "^0.0.2",
    "klapuch/output": "^2.12.1",
    "klapuch/storage": "^0.0.1",
    "klapuch/sql": "^0.0.1",
    "klapuch/validation": "^2.6.3",
    "klapuch/iterator": "^1.1.2",
    "klapuch/routing": "^0.0.14",
    "klapuch/dataset": "^0.0.1",
    "klapuch/scheduling": "^0.0.1",
    "klapuch/encryption": "^2.1",
    "tracy/tracy": "^2.5",
    "justinrainbow/json-schema": "^5.2",
    "nette/utils": "^2.5"
  },
  "require-dev": {
    "nette/tester": "^2.0",
    "jakub-onderka/php-parallel-lint": "^1.0.0",
    "slevomat/coding-standard": "^4.8.3",
    "sebastian/phpcpd": "^4.0.0",
    "consistence/coding-standard": "^3.0",
    "phpstan/phpstan": "^0.11.0",
    "phpstan/phpstan-strict-rules": "*",
    "mockery/mockery": "^1.0.0",
    "klapuch/http": "^1.4",
    "pepakriz/phpstan-exception-rules": "*"
  },
  "autoload": {
    "psr-4": {
      "Bulletpoint\\": [
        "App/"
      ]
    },
    "files": [
      "App/Http/statuses.php",
      "www/translations.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Bulletpoint\\": [
        "Tests/"
      ]
    }
  }
}
